{
  "name": "lead-the-way",
  "version": "0.1.0",
  "proxy": "http://localhost:5000",
  "private": true,
  "dependencies": {
    "bootstrap": "4.1.2",
    "font-awesome": "^4.7.0",
    "jquery": "3.3.1",
    "popper.js": "1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-media": "1.8.0",
    "react-scripts": "2.0.4",
    "reactstrap": "6.3.1"
  },
  "scripts": {
    "start": "npm-run-all -p sass:watch start-js",
    "sass:watch": "npm run sass:build && node-sass-chokidar -w src/styles/scss -o src/styles/css",
    "sass:build": "node-sass-chokidar src/styles/scss -o src/styles/css --source-map true",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
